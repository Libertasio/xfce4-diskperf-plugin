#: ../panel-plugin/main.c:236
#, c-format
msgid ""
"%s\n"
"----------------\n"
"I/O    (MiB/s)\n"
"  Read :%3.2f\n"
"  Write :%3.2f\n"
"  Total :%3.2f\n"
"Busy time (%c)\n"
"  Read : %3d\n"
"  Write : %3d\n"
"  Total : %3d"
msgstr "%s\n----------------\nI/O (MiB/s)\nBaca :%3.2f\nTulis :%3.2f\nJumlah :%3.2f\nMasa sibuk (%c)\nBaca : %3d\nTulis : %3d\nJumlah : %3d"

