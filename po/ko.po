msgid "I/O transfer"
msgstr "입출력 전송"

#: ../panel-plugin/config_gui.c:181
msgid "MiB transferred / second"
msgstr "전송된 MiB / 초"

#: ../panel-plugin/config_gui.c:187
msgid "Busy time"
msgstr "사용 시"

#: ../panel-plugin/config_gui.c:191
