#, c-format
msgid "%s: Device statistics unavailable."
msgstr "%s: gaitu estatistika eskuraezinak"

#: ../panel-plugin/main.c:219
#, c-format
msgid ""
"%s\n"
"----------------\n"
"I/O    (MiB/s)\n"
"  Read :%3.2f\n"
"  Write :%3.2f\n"
"  Total :%3.2f\n"
"Busy time (%c)\n"
"  Read : %3d\n"
"  Write : %3d\n"
"  Total : %3d"
msgstr ""
"%s\n"
"----------------\n"
"S/I    (MiB/s)\n"
"  Iraku :%3.2f\n"
"  Idatz :%3.2f\n"
"  Guzt :%3.2f\n"
"Lan denb (%c)\n"
"  Iraku : %3d\n"
"  Idatz : %3d\n"
"  Guzt : %3d"

