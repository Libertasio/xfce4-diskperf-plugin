msgid "Bar color "
msgstr "Farba panelu"

#: ../panel-plugin/config_gui.c:250 ../panel-plugin/config_gui.c:316
#: ../panel-plugin/config_gui.c:325
msgid "Press to change color"
msgstr "Stlačiť pre zmenu farby"

