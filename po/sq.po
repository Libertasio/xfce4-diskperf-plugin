msgid "Read-Write"
msgstr "Lexim-Shkrim"

#: ../panel-plugin/config_gui.c:292
msgid "\"Read\" monitor first"
msgstr "Mbikëqyrës \"Leximi\" së pari"

#: ../panel-plugin/config_gui.c:299
msgid "Write-Read"
msgstr "Shkrim-Lexim"

#: ../panel-plugin/config_gui.c:304
